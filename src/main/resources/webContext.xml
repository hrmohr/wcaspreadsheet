<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="
    http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="scoresheetAction" class="dk.cubing.wcaspreadsheet.action.ScoresheetAction">
		<property name="spreadSheetFilename" value="/xls/results.xls" />
	</bean>
	
	<bean id="scramblesAction" class="dk.cubing.wcaspreadsheet.action.ScramblesAction">
		<constructor-arg index="0" value="/xsl/scrambles.xsl" />
	</bean>

</beans>
